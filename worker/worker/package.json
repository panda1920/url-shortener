{
  "name": "worker",
  "version": "1.0.0",
  "description": "worker",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "posttest": "npm run lint",
    "docker:build": "docker build -t worker .",
    "docker:run": "docker run -p 3000:3000 -d worker",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.3.3",
    "@loopback/context": "^3.4.0",
    "@loopback/core": "^2.8.0",
    "@loopback/openapi-v3": "^3.1.3",
    "@loopback/repository": "^2.7.0",
    "@loopback/rest": "^5.1.1",
    "@loopback/rest-explorer": "^2.0.4",
    "@loopback/service-proxy": "^2.0.4",
    "loopback-connector-kv-redis": "^3.0.3",
    "loopback-connector-mongodb": "^4.2.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.0.1",
    "source-map-support": "^0.5.16",
    "@loopback/testlab": "^3.0.1",
    "@types/node": "^10.17.19",
    "@loopback/eslint-config": "^6.0.3",
    "eslint": "^6.8.0",
    "typescript": "~3.8.3"
  }
}
